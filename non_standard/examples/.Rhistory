ExperienceBasis$qxt <- function(x=0, time=0, ...) {#
	out< rep(NA, length(time))#
	for (j in 1:length(time)) {#
		t <- time[j]#
		if(t==0) {#
			out[j] <- NA#
			} else {#
				out[j] <- 0.010 + 0.001*t#
		}#
	}#
	return(out)#
}
