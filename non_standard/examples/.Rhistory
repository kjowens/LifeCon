RGD<-function(tbl,k)#
                {#
                    n<-length(tbl[,1]);#
                    r<-as.array(runif(k));#
                    for (l in 1:k)#
                    {#
                        u<-r[l];#
                        i<-ceiling(n*u);#
                        if (u>=tbl[i,1]) {#
                            r[l]=i;#
                        }#
                        else {#
                            r[l]=tbl[i,2];#
                            }#
                    }#
                    r#
                }
